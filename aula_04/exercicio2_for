const leia = require('readline-sync')

let pares = 0
let impares = 0

for (let i = 1; i <= 10; i++){
    let numero = leia.questionInt("Digite um numero: ")
    if(numero % 2 === 0)
        pares++
else{
    impares++
    }

}

console.log("O total de números pares é: ", pares)
console.log("O total de números ímpares é: ", impares)